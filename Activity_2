# Activity 2: Polymorphism Challenge
class Vehicle:
    def move(self):
        pass  # Placeholder method to be overridden

class Car(Vehicle):
    def move(self):
        print("ğŸš— Driving on the road")

class Plane(Vehicle):
    def move(self):
        print("âœˆï¸ Flying in the sky")

class Boat(Vehicle):
    def move(self):
        print("ğŸš¢ Sailing on water")

# Using polymorphism
vehicles = [Car(), Plane(), Boat()]
for v in vehicles:
    v.move()

